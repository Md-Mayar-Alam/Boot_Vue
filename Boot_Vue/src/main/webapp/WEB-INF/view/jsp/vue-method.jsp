<html>
	<head>
		<script src="https://cdn.jsdelivr.net/npm/vue@2.6.0"></script>
	</head>
	
	<body>
		<h1>Welcome to vue-method example</h1>
		<div id="vue-method">
			<h1>FirstName: {{ firstName }}</h1>
			<h2>LastName: {{ lastName }}</h2>
			<h3>Address: {{ address }}</h3>
			<h4>My Details: {{ mydetails() }}</h4>
		</div>
		<script src="/js/vue-method.js"></script>
	</body>
</html>